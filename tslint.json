{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-no-unused-expression-chai"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": true,
    "max-classes-per-file": [
      false
    ],
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "max-line-length": {
      "options": [
        100
      ]
    },
    "variable-name": [
      true,
      "allow-leading-underscore"
    ]
  },
  "rulesDirectory": []
}