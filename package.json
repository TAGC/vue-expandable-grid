{
  "name": "vue-expandable-grid",
  "version": "0.2.0-2",
  "license": "MIT",
  "author": "TAGC",
  "main": "dist/vue-expandable-grid.common.js",
  "types": "dist/typings/index.d.ts",
  "description": "Vue component representing an infinitely-expandable grid",
  "repository": {
    "type": "git",
    "url": "https://github.com/TAGC/vue-expandable-grid"
  },
  "bugs": {
    "url": "https://github.com/TAGC/vue-expandable-grid/issues"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:types",
    "build:lib": "vue-cli-service build --target lib src/index.ts",
    "build:types": "tsc -p tsconfig.types.json && cp -R typings/* dist/typings",
    "build:demo": "cross-env DEMO=1 vue-cli-service build --target app --name 'vue-expandable-grid' --dest demo/dist demo/src/main.ts",
    "lint": "vue-cli-service lint",
    "dev": "cross-env DEMO=1 vue-cli-service serve demo/src/main.ts",
    "test": "vue-cli-service test:unit",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist demo/dist"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "vue-resize": "^0.4.4",
    "vue-property-decorator": "^6.1.0",
    "vue-virtual-collection": "^1.2.3"
  },
  "peerDependencies": {
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/lodash": "^4.14.109",
    "@types/mocha": "^5.2.2",
    "@vue/cli-plugin-babel": "^3.0.0-rc.1",
    "@vue/cli-plugin-typescript": "^3.0.0-rc.1",
    "@vue/cli-plugin-unit-mocha": "^3.0.0-rc.1",
    "@vue/cli-service": "^3.0.0-rc.1",
    "@vue/test-utils": "^1.0.0-beta.18",
    "ajv": "^6.5.1",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "node-sass": "^4.9.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "tslint": "^5.10.0",
    "tslint-no-unused-expression-chai": "^0.1.3",
    "typescript": "^2.9.2",
    "vue": "^2.5.16",
    "vue-material": "^1.0.0-beta-10.2",
    "vue-template-compiler": "^2.5.16"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "files": [
    "dist/"
  ]
}
